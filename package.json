{
  "name": "votefromabroad",
  "version": "1.0.0",
  "description": "VoteFromAbroad.org -- Absentee Ballots for US Citizens Abroad",
  "author": "Alex Montgomery <Alex@DemocratsAbroad.org>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "getaddressdata": "rm -rf data/postal && svn export https://github.com/mirumee/google-i18n-address/trunk/i18naddress/data ./data/postal",
    "build": "nuxt build",
    "build:lambda": "netlify-lambda build src/lambda",
    "start": "nuxt start",
    "start:lambda": "netlify-lambda serve src/lambda",
    "generate": "nuxt generate",
    "updateReleaseOnSentry": "bash ./sentry.sh",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/vue-fontawesome": "0.0.21",
    "@nuxtjs/axios": "^4.5.2",
    "@nuxtjs/proxy": "^1.1.4",
    "@nuxtjs/pwa": "^2.0.5",
    "@nuxtjs/sentry": "^1.0.3",
    "auth0-js": "^9.6.0",
    "aws-amplify": "^0.3.4",
    "babel-polyfill": "^6.26.0",
    "buefy": "^0.6.6",
    "cleave.js": "^1.3.7",
    "current-device": "^0.7.8",
    "flag-icon-css": "^2.9.0",
    "jwt-decode": "^2.2.0",
    "libphonenumber-js": "^0.4.48",
    "mailcheck": "^1.1.1",
    "netlify-lambda": "^0.2.0",
    "nuxt": "^1.4",
    "nuxt-i18n": "^2.10.0",
    "nuxtent": "^1.4.1",
    "snarkdown": "^1.2.2",
    "tracking": "^1.1.3",
    "vue-intercom": "^1.0.12",
    "vue-markdown": "git+https://github.com/apmontgomery/vue-markdown.git",
    "vuelidate": "^0.7.4",
    "vuex-persist": "1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  }
}
